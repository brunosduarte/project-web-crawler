{
  "name": "@project-web-crawler/server",
  "author": "Bruno S. Duarte",
  "version": "0.0.1",
  "description": "Generate a complete sitemap of a specific domain",
  "private": true,
  "license": "MIT",
  "main": "src/infrastructure/app.ts",
  "scripts": {
    "dev": "tsx watch src/infrastructure/app.ts",
    "start": "node --watch ./src/infrastructure/app.ts",
    "lint": "eslint src --ext .ts --fix",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:watch": "vitest",
    "test:coverage": "vitest --coverage",
    "test:debug": "node --experimental-vm-modules --inspect-brk node_modules/.bin/jest --injectGlobals false --runInBand --watchAll test/"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.3",
    "p-queue": "^6.6.2",
    "puppeteer": "^22.0.0",
    "tsx": "^4.7.1"
  },
  "devDependencies": {
    "@enkidevs/eslint-config-backend": "^12.0.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.29",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@vitest/coverage-v8": "^1.4.0",
    "@vitest/ui": "^1.4.0",
    "eslint": "^8.0.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
    "eslint-plugin-promise": "^6.0.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.4",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.0",
    "typescript": "^5.4.2",
    "vitest": "^1.3.1"
  }
}
